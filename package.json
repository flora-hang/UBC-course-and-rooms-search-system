		"@types/express": "4.17.13",
		"@types/fs-extra": "9.0.13",
		"@types/mocha": "10.0.1",
		"@types/node": "18.11.18",
		"@typescript-eslint/eslint-plugin": "5.48.0",
		"@typescript-eslint/parser": "5.48.0",
		"@ubccpsc310/folder-test": "3.0.0",
